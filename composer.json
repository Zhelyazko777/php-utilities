{
    "name": "zhelyazko777/php-utilities",
    "description": "Simple object mapper for Laravel",
    "keywords": [
        "zhelyazko777",
        "laravel",
        "php-utilities"
    ],
    "homepage": "https://github.com/zhelyazko777/php-utilities",
    "license": "MIT",
    "authors": [
        {
            "name": ":Zhelyazko Georgiev",
            "email": "zhelyazko777@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Zhelyazko777\\Utilities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhelyazko777\\Utilities\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
